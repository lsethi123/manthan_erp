.row
  .col-sm-3
    %h4 Meal Types
  .col-sm-5
  .col-sm-3
    %input#search{name: "search", placeholder: "Enter Search Term...", type: "text",:class => 'form-control'}
%br
.row
  .col-sm-2
  .col-sm-6
    %table#tblData.target.table.table-striped.table-condensed.table-bordered
      %thead
        %tr
          %th Meal Type
          %th time
        
         
      %tbody
        - @mealtypes.each do |mealtype|
          %tr
            %td= mealtype.meal_type
            %td= mealtype.time
          
          

      
