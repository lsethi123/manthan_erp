.col-sm-9
  .row{:ng_controller => "RouteController"}
    .col-sm-6
      %h4 Students Belongs to this Route
      %table.table.table-bordered
        %thead
          %tr
            %th Student id
            %th Student Name
              
             
        %tbody
          %tr{"ng-repeat" => "student in students"}
            %td {{student.id}}
            %td {{student.name}}
            
    
