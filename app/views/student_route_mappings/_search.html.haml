= form_tag student_route_mappings_path, :method => 'get' , :class => "form-inline" do
  .row{"ng-controller" => "Mapping1Controller"}
    .col-sm-5
      = autocomplete_field_tag :search_term,
      params[:search_term],
      auto_search_autocomplete_student_master_name_path,
      :placeholder => "Student Name", :class => 'form-control',
      :id_element => "#search"
      = hidden_field_tag :id, :id => "search"
     
    .col-sm-6
      = link_to "Search", "#", :class => "btn btn-primary", :ng_click => "searchStudents(id)"
  
