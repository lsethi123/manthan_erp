%form{:class=> "form-horizantal", :ng_submit => "update(fee_type)", :id => "updateFeeType"}
  .col-md-12.form-group
    = label_tag "Fee Type", nil, :class => "control-label col-md-4"
    .col-md-5
      = text_field_tag "fee_type", "fee_type.fee_type", :class => "form-control", :ng_model => "fee_type.fee_type"
  .row
    .col-md-5.col-md-offset-3
      %button{:type=>"submit", :class=> "btn btn-primary"}=  "Update"

