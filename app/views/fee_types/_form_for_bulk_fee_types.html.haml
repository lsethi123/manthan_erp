= form_tag create_bulk_fee_types_path, :method => "POST", :class=> "form-horizantal" do 
  .row{"ng-repeat" => "fee_type in fee_types"}
    .form-group
      = label_tag "Fee Type {{$index+1}}", nil, :class => "form-label col-md-2"
      .col-md-5
        = text_field_tag "bulk_fee_types[][fee_type]",nil, :class => "form-control"
  .row
    .col-md-5.col-md-offset-3
      %button{:type=>"submit", :class=> "btn btn-primary"}=  "Save"
      %button{:type => "button", :class=> "btn btn-primary", :ng_click => "addMorefeeTypes()"}=  "More Fee Types"
      = link_to "Back", fee_types_path, :class => "btn btn-primary"
