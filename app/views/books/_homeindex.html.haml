.panel.panel-default
  .test
    .col-sm-4
      .pull-left
        .input-group
          %span.input-group-addon
            = fa_icon "search lg"
          %input#search{name: "search", placeholder: "Enter Search Term", type: "text",:class => 'form-control'}
          %br
    .col-sm-4
      %center
        %h3
          Books
.row
  %table#tblData.target.table.table-striped.table-condensed.table-bordered
    %thead
      %tr
        %th Name
        %th ISBN Number
        %th Author
        %th Year Of Publishing
        %th Date Purchased
        %th Type
    %tbody
      - @books.each do |book|
        %tr
          %td= book.name
          %td= book.isbn
          %td= book.author
          %td= book.year_of_publishing
          %td= book.created_at
          %td= book.book_type
          
      
