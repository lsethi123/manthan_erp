%table.table.table-striped.table-condensed.table-bordered
  %thead
    %tr
      %th Title:
      %th Date:
      %th Description
  %tbody
    - Add.all.each do |add|
      %tr
        %td= add.title
        %td= add.date
        %td= add.description
       
%center
  = link_to "back", new_add_path, class: "btn btn-primary"
