.panel.panel-default
  .panel-heading
    .pull-left
      %input#search{name: "search", placeholder: "Enter Search Term...", type: "text", :class => 'form-control'}/
    .pull-right
      %br
      %br
    %center
      %h4
        Assessments Planned

%table#tblData.target.table.table-striped.table-condensed.table-bordered
  %thead
    %tr
      %th Form no
      %th Title
      %th Description
      %th Staff
      %th Grade
      %th Start Time
      %th End Time
        
  %tbody
    - @admissions.each do |admission|
      %tr
        %td= link_to "#{admission.form_no}", home_index_admission_path(admission)
        %td= admission.title
        %td= admission.description
        %td= admission.grade_master.grade_name
        %td= admission.klass
        %td= admission.start_time
        %td= admission.end_time
        
