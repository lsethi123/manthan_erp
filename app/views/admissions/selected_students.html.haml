.row
  .col-sm-3
    .pull-left
      .input-group
        %span.input-group-addon
          = fa_icon "search lg"
        %input#search{name: "search", placeholder: "Enter Search Term...", type: "text",:class => 'form-control'}/
        %br
  .col-sm-4
    %center
      %h4
        List of Selected Forms
%br
%br

%table#tblData.target.table.table-striped.table-condensed.table-bordered{:style => "width:95%"}
  %thead
    %tr
      %th Form no
      %th Name
      %th Class
      %th Status
        
  %tbody
    - @admissions.each do |admission|
      %tr
        %td{:style => "width:25%"}
          = link_to "#{admission.form_no}", home_index_admission_path(admission)
        %td{:style => "width:25%"}
          = admission.name
        %td{:style => "width:25%"}
          = admission.grade
        %td{:style => "width:25%"}
          = admission.status

