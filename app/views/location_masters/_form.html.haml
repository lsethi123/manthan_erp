= form_for LocationMaster.new , :html => { :class => "form-horizontal", :role => "form"} do |f|
  .row
    .col-sm-3
      = f.label "location", :class => "form-label"
    .col-sm-2
      = f.text_field "location_name", :class => "form-control"
    .col-sm-2
      %button{:type=>"submit", :class=> "btn btn-primary"}=  "Save"
