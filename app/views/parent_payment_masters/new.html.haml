.col-md-12
  %student_header_details{ :student_id => "#{@parent_payment_master.student_id}"}
.col-md-12
  %h4.panel-title 
    = "Fee Details"
    = link_to (fa_icon "angle-double-up lg", :id => "collapse-icon"),"#" ,:class => "collapse-link pull-right", :data => {:toggle => "collapse", :target => "#collapse-div0"}
  %hr
  #collapse-div0.col-md-12.collapse
    .col-md-5
      %grade_fee_details{ :student_id => "#{@parent_payment_master.student_id}"}
    .col-md-7
      %payment_type_tabs{ :student_id => "#{@parent_payment_master.student_id}", :paymentMasterId => "#{@parent_payment_master.id}"}
  
.col-md-12{:ng_controller => "PaymentsController"}
  %h4.panel-title 
    = "Fee Payment"
  %hr
    = render "form"
