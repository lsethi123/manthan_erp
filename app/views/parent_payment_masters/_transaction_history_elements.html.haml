.panel.panel-default
  .panel-heading
    Transactions
  %table.table.table-bordered.table-striped.table-hover
    %thead
      %th Transaction Date
      %th Amount
      %th Type
      %th Particulars
    %tbody
      %tr{"ng-repeat" => "transaction in parent_payment_transactions"}
        %td {{transaction.transaction_date}}
        %td {{transaction.amount_in_rupees}}
        %td {{transaction.transaction_type}}
        %td {{transaction.particulars}}
